{
  "name": "seancoughlin.me",
  "description": "My personal website",
  "author": "Sean Coughlin",
  "license": "MIT",
  "version": "1.0.0",
  "homepage": "https://seancoughlin.me",
  "engines": {
    "yarn": ">1.22.0",
    "node": ">20.0.0"
  },
  "scripts": {
    "start": "vite dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:playwright": "npx playwright test --project chromium --project firefox --project webkit --project mobileChrome --project mobileSafari --project googleChrome",
    "test:lighthouse": "npx playwright test --project lighthouse",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint --ext .js,.svelte,.ts src/ tests/",
    "lint-fix": "eslint --fix --ext .js,.svelte,.ts src/ tests/",
    "lint-staged": "lint-staged",
    "pretty-check": "prettier --check .",
    "pretty-write": "prettier --write .",
    "lighthouse": "lighthouse https://seancoughlin.me --output-path=./lighthouse-report",
    "husky": "husky install",
    "predeploy": "vitest run --coverage && vite build",
    "deploy": "gh-pages -d build",
    "dependency-analysis": "npx depcheck"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --fix"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.4.3",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/svelte": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/coverage-v8": "^1.1.3",
    "autoprefixer": "^10.4.16",
    "axe-playwright": "^1.2.3",
    "eslint": "^8.56.0",
    "eslint-plugin-playwright": "^0.21.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-vitest": "^0.3.20",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "playwright-lighthouse": "^4.0.0",
    "postcss": "^8.4.33",
    "prettier": "3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-static-copy": "^1.0.0",
    "vitest": "^1.1.3"
  },
  "type": "module"
}
