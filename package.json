{
  "name": "seancoughlin.me",
  "description": "My personal website",
  "author": "Sean Coughlin",
  "license": "MIT",
  "version": "1.0.0",
  "homepage": "https://seancoughlin.me",
  "scripts": {
    "start": "bunx --bun vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview --open",
    "test": "vitest run --coverage",
    "test:playwright": "bunx playwright test --project firefox --project webkit --project mobileChrome --project mobileSafari --project googleChrome",
    "test:lighthouse": "bunx playwright test --project lighthouse",
    "test:smoke": "bunx playwright test --project smoke",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "lint-staged": "lint-staged",
    "pretty-check": "prettier --check .",
    "pretty-write": "prettier --write .",
    "lighthouse": "lighthouse https://seancoughlin.me --output-path=./lighthouse-report",
    "husky": "husky install",
    "predeploy": "vitest run --coverage && vite build",
    "deploy": "gh-pages -d build",
    "dependency-analysis": "bunx depcheck",
    "update-deps": "bunx npm-check-updates --root --format group -i"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.43.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.1.0",
    "@vitest/coverage-v8": "^1.4.0",
    "autoprefixer": "^10.4.19",
    "axe-playwright": "^2.0.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-playwright": "^1.5.4",
    "eslint-plugin-svelte": "^2.36.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vitest": "^0.5.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lighthouse": "^11.7.1",
    "lint-staged": "^15.2.2",
    "playwright": "^1.43.0",
    "playwright-lighthouse": "^4.0.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.6.0",
    "vite": "^5.2.8",
    "vite-plugin-static-copy": "^1.0.2",
    "vitest": "^1.4.0"
  },
  "type": "module"
}